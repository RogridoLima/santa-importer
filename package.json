{
  "name": "santa-importer",
  "displayName": "Santa Importer",
  "description": "Get a hold of your caffeine addiction and format your imports!",
  "version": "1.0.0",
  "repository": {
    "url": "https://github.com/RogridoLima/santa-importer.git"
  },
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:santa-importer.formatImports"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "santa-importer.formatImports",
        "title": "Xmas Imports ASAP"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.64.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "typescript": "^4.5.4",
    "@vscode/test-electron": "^2.0.3"
  }
}
